{
    "name": "alexandria-lib-server",
    "version": "0.7.5",
    "author": "dev.goraebap",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "start:dev": "cross-env NODE_ENV=development nest start --watch",
        "start:prod": "cross-env NODE_ENV=production node dist/main",
        "db:generate": "cross-env NODE_ENV=development drizzle-kit generate --config=./src/shared/database/drizzle.config.ts",
        "db:migrate": "cross-env NODE_ENV=development drizzle-kit migrate --config=./src/shared/database/drizzle.config.ts",
        "db:studio": "drizzle-kit studio"
    },
    "dependencies": {
        "@libsql/client": "^0.14.0",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.3.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/swagger": "^8.0.7",
        "@scalar/nestjs-api-reference": "^0.3.177",
        "axios": "^1.7.9",
        "bcrypt": "^5.1.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.7",
        "drizzle-orm": "^0.38.3",
        "handlebars": "^4.7.8",
        "jsonwebtoken": "^9.0.2",
        "nanoid": "^3.0.0",
        "nodemailer": "^6.9.16",
        "reflect-metadata": "^0.2.0",
        "rxjs": "^7.8.1",
        "sqlite3": "^5.1.7"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/bcrypt": "^5.0.2",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.2",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/node": "^20.3.1",
        "@types/supertest": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "drizzle-kit": "^0.30.1",
        "eslint": "^8.0.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.5.0",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
